import random as rd
import matplotlib.pyplot as plt
import numpy as np
def aleatoire(L):
    return rd.randint(0,L)


def distances(n,L):
    D=np.array([[0 for i in range(n)] for i in range(n)])
    
    for i in range(n):
        for j in range(i,n):
            if i!=j: 
                D[i][j]=aleatoire(L)
                
    for i in range(n):
        for j in range(0,i):
            D[i][j]=D[j][i]
            
    print(D)
    
    
    
